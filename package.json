{
  "name": "rollup-obfuscator",
  "version": "2.0.0",
  "description": "A plugin to obfuscate javascript for rollup based on https://www.npmjs.com/javascript-obfuscator",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:tsup": "tsup-node --watch",
    "dev:rollup": "rollup -w -c dev/rollup.config.js",
    "build": "tsup-node"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^17.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.38.4",
    "tsup": "^5.1.0",
    "typescript": "^4.4.3"
  },
  "license": "MIT",
  "keywords": [
    "obfuscator",
    "rollup",
    "rollup obfuscator",
    "rollup plugin",
    "plugin",
    "rollup plugin obfuscator",
    "rollup-obfuscator",
    "javascript obfuscator"
  ],
  "homepage": "https://github.com/ghostdevv/rollup-obfuscator",
  "repository": {
    "type": "git",
    "url": "https://github.com/ghostdevv/rollup-obfuscator"
  },
  "bugs": {
    "url": "https://github.com/ghostdevv/rollup-obfuscator"
  },
  "dependencies": {
    "javascript-obfuscator": "^4.x"
  }
}